{
  "name": "templ-demo",
  "version": "1.0.0",
  "dependencies": {
    "@react-three/drei": "^9.0.0",
    "@react-three/fiber": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-loader-spinner": "^5.0.0",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "esbuild": "^0.19.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.11"
  },
  "scripts": {
    "build": "esbuild --bundle web/index.js --outdir=web/static/dist --minify --global-name=bundle",
    "dev": "esbuild --bundle web/index.js --outdir=web/static/dist --watch --global-name=bundle",
    "build-css": "postcss input.css -o web/static/assets/styles.css",
    "dev-css": "postcss input.css -o web/static/assets/styles.css --watch"
  }
}
